{
  "extends": ["plugin:prettier/recommended", "next/core-web-vitals"],
  "plugins": ["prettier"],
  "rules": {
    "react-hooks/rules-of-hooks": "warn",
    "react/display-name": "warn",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": false
      }
    ],
    "no-new-object": "warn",
    "prefer-destructuring": "warn",
    "keyword-spacing": "warn"
  }
}
